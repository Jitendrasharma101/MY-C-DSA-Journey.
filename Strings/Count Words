#include <iostream>
#include <cstring>
using namespace std;

int countWords(string input) {
    int i;
    int count=0;
    for(i=0;i<input.length()-1;i++){
        if(input[i]==' '){
            count++;
        }
        
    }return count+1;
}

int main() {
    string input;
    getline(cin, input);
    int output = countWords(input);
    cout << output << endl;
    return 0;
}
