#include <bits/stdc++.h> 
#include <iostream>
#include<algorithm>

using namespace std;

int removeduplicate(int arr[] ,int n){
    int i;
    int j=0;
    int temp[n];
    for(i=0;i<n-1;i++){
        if(arr[i]!=arr[i+1]){
            temp[j++]=arr[i];
        }
    }temp[j++]=arr[n-1];
    for(i=0;i<j;i++){
        arr[i]=temp[i];
    }
    return j;
}
void kthSmallestLargest(int arr[], int n, int k) {
    cout<<arr[n-k]<<" "<<arr[k-1]<<endl;
    
}

int main() {
    int t;
    cin >> t;

    while (t > 0) {
        int n, k;
        cin >> n >> k;
        int arr[n];
        for (int i = 0; i < n; i++) {
            cin >> arr[i];
        }
        if(k>n){
            cout<<"-1"<<" "<<"-1"<<endl;
        }else{
            int s=sizeof(arr)/sizeof(arr[0]);
        sort(arr,arr+s);
        n=removeduplicate(arr,s);
        kthSmallestLargest(arr, n, k);
        }
        
        t--;
    }
    return 0;
}
