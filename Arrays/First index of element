#include <iostream>
using namespace std;

int main() {
    int N;
    cin>>N; 
    int arr[N],x;
    for(int i=0;i<N;i++){
        cin>>arr[i];
    }
    cin>>x;
    int i;
    for(i=0;i<N;i++){
        if(arr[i]==x){
            cout<<i;
            break;
        }
         
    }if(arr[i]!=x){
             cout<<-1;
         }
    

    return 0;
}
