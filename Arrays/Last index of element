#include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n;
    int i;
    int arr[n];
    for(i=0;i<n;i++){
        cin>>arr[i];
        
    }
    int r=-1;
    int x;
    cin>>x;
    for(i=0;i<n;i++){
        if(arr[i]==x){
            r=i;
        }
    }
    
    cout<<r;
    return 0;
}
