#include<iostream>
#include<string.h>
using namespace std;
int main(){
    string s;
    cout<<"Enter the string   ";
    getline(cin,s);
    int low=0;
    int high=s.size()-1;
    int flag=0;
    while(low<high){
        if(s[low]!=s[high]){
            flag=1;

        }
        low++;
        high--;
    }if(flag==0){
        cout<<"given string is palindrome"<<endl;;

    }else{
        cout<<"given string is not palindrome"<<endl;
        ;
    }
   return 0;

}
